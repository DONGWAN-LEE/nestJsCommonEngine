# Server Configuration
SERVER_NAME=LOCAL
NODE_ENV=development

# MySQL Database Configuration
# true: MySQL 데이터베이스 사용, false: 데이터베이스 비활성화
ENABLE_DATABASE=false
MASTER_DATABASE_HOST=localhost
MASTER_DATABASE_PORT=3306
MASTER_DATABASE_NAME=your_database_name
MASTER_DATABASE_USERNAME=your_username
MASTER_DATABASE_PASSWORD=your_password
TYPEORM_LOGGING=true

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWD=

# Cache Configuration
CACHE_CHECK_STR=your_cache_check_string

# Encryption
ENCRYPTION_KEY=your_encryption_key
CRYPTO_PASSWD=your_crypto_password

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
API_EXTERNAL_URL=your_api_external_url

# Google Services
GOOGLE_SHEET_ID=your_google_sheet_id
GOOGLE_DATABASE_SHEET_ID=your_google_database_sheet_id

# Aladin API
ALADIN_TTB_KEY=your_aladin_ttb_key
ALADIN_API_URL=your_aladin_api_url

# Treenod API
TREENOD_API_URL=your_treenod_api_url
TREENOD_API_KEY=your_treenod_api_key 